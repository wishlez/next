generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id       Int    @id @default(autoincrement())
    login    String @unique @db.VarChar(64)
    name     String @db.VarChar(64)
    password String @db.VarChar(64)
}
